import java.io.*;
import java.util.*;

public class friday {
  public static void main(String[] args) throws IOException {
		Scanner scanner = new Scanner(new FileReader("friday.in"));
		PrintWriter pw = new PrintWriter(new FileWriter("friday.out"));
		int N = scanner.nextInt();
		int firstDayWeekdayIndex = 1;
		int[] weekDayCounts = new int[8];
		int year = 1900;
		int month = 1;
		while (N-- > 0) {
			for(month=1;month<=12;month++){
				firstDayWeekdayIndex = getIndex(12,firstDayWeekdayIndex);
				weekDayCounts[firstDayWeekdayIndex]++;
				if(month!=12){
					firstDayWeekdayIndex = getIndex(getMonthDays(year,month)-13+1,firstDayWeekdayIndex);
				}else{
					firstDayWeekdayIndex = getIndex(31-13+1,firstDayWeekdayIndex);
				}
			}
		}
		for(int i=0;i<7;i++){
			pw.println(weekDayCounts[(i+6)%8]);
		}
		scanner.close();
		pw.close();
		System.exit(0);
	}
	public static int getMonthDays(int year,int month){
		switch(month){
		case 1:
		case 3:
		case 5:
		case 7:
		case 8:
		case 10:
		case 12:return 31;
		case 2:
			if(isLeap(year)){
				return 29;
			}
			return 28;
		default:
			return 30;
		}
	}
	public static boolean isLeap(int year){
		if(year%100==0){
			if(year%400==0){
				return true;
			}else{
				return false;
			}
		}else if(year%4==0){
			return true;
		}else{
			return false;
		}
		
	}
	public static int getIndex(int days,int index) {
		int plus = days - (days / 7) * 7;
		return (index + plus )%7;
	}
}
